        "outDir": "./dist/",
        "esModuleInterop": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "target": "esnext",
        "jsx": "react",
        "baseUrl": ".",
